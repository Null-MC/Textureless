{
  "multipart": [
    {
      "apply": [
        {
          "model": "block/mangrove_fence_post"
        },
        {
          "model": "block/mangrove_fence_post",
          "y": 90
        },
        {
          "model": "block/mangrove_fence_post",
          "y": 180
        },
        {
          "model": "block/mangrove_fence_post",
          "y": 270
        }
      ]
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "block/mangrove_fence_side_north"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "block/mangrove_fence_side_east"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "block/mangrove_fence_side_south"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "block/mangrove_fence_side_west"
      }
    }
  ]
}